{
  "name": "permawebjs",
  "version": "0.0.96",
  "description": "Utility library to build full stack permaweb applications",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "esnext",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.js.map",
    "dist/lib/*.js",
    "dist/lib/*.d.ts",
    "dist/lib/*.js.map"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest --config jestconfig.json",
    "test:wallet": "jest --testPathPattern=wallet",
    "test:serverless": "jest --testPathPattern=serverless",
    "test:transaction": "jest --testPathPattern=transactions",
    "lint": "tslint -p  tsconfig.json",
    "format": "prettier --write src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labscommunity/permawebjs.git"
  },
  "author": "CommunityLabs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/labscommunity/permawebjs/issues"
  },
  "homepage": "https://github.com/labscommunity/permawebjs#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node-fetch": "^2.6.2",
    "arlocal": "^1.1.60",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.10.3",
    "@execution-machine/sdk": "^0.1.6",
    "arweave": "^1.13.0",
    "arweave-mnemonic-keys": "^0.0.9",
    "warp-contracts": "^1.2.52"
  },
  "exports": {
    "./auth": "./dist/lib/auth.js",
    "./wallet": "./dist/lib/wallet.js",
    "./serverless": "./dist/lib/serverless.js",
    "./transaction": "./dist/lib/transaction.js",
    "./contract": "./dist/lib/contract.js"
  },
  "typesVersions": {
    "*": {
      "auth": [
        "dist/lib/auth.d.ts"
      ],
      "wallet": [
        "dist/lib/wallet.d.ts"
      ],
      "serverless": [
        "dist/lib/serverless.d.ts"
      ],
      "transaction": [
        "dist/lib/transaction.d.ts"
      ],
      "contract": [
        "dist/lib/contract.d.ts"
      ]
    }
  }
}
